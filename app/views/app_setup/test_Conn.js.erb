var1 = $("#app_name").val();
var2 = $('input[name="environment"]:checked').val()
var3 = $('input[name="rest_method"]:checked').val()
var4 = $("#uri").val();
var5 = $("#req_hdr").val();
var6 = $("#req_json").val();
var request = $.ajax({
    url: '/application_details/test_Conn',
    type: 'post',
    data: $.param({application_detail:{name:var1, environment:var2, rest_method:var3, uri:var4, request_hdr:var5, request_text:var6}}),
    success: function(jsonResponse){
    if(jsonResponse.status == "200"){
      $("#time_div").html(jsonResponse.html);
    }}
  });
	